{
    "npmScope": "whatsapp-clone",
    "affected": {
        "defaultBase": "main"
    },
    "implicitDependencies": {
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        ".eslintrc.json": "*"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/nx-cloud",
            "options": {
                "cacheableOperations": ["build", "lint", "test", "e2e"],
                "accessToken": "ODkyZjMzNGEtZGIxOC00MTkyLWE5MWYtNGQ4NGFiZWEwYWM1fHJlYWQtd3JpdGU="
            }
        }
    },
    "targetDependencies": {
        "build": [
            {
                "target": "build",
                "projects": "dependencies"
            }
        ]
    },
    "projects": {
        "data": {
            "tags": ["scope:model"]
        },
        "ui": {
            "tags": ["scope:common-components"]
        },
        "whatsapp-backend": {
            "tags": ["scope:whatsapp-backend"]
        },
        "whatsapp-ui": {
            "tags": ["scope:whatsapp-ui"]
        }
    }
}
