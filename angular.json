        "util": {
            "root": "libs/util",
            "sourceRoot": "libs/util/src",
            "projectType": "library",
            "architect": {
                "build": {
                    "builder": "@nrwl/workspace:tsc",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/libs/util",
                        "main": "libs/util/src/index.ts",
                        "tsConfig": "libs/util/tsconfig.lib.json",
                        "assets": ["libs/util/*.md"]
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/util/**/*.ts"]
                    },
                    "outputs": ["{options.outputFile}"]
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/util"],
                    "options": {
                        "jestConfig": "libs/util/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "whatsapp-backend": {
            "root": "apps/whatsapp-backend",
            "sourceRoot": "apps/whatsapp-backend/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/node:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/whatsapp-backend",
                        "main": "apps/whatsapp-backend/src/main.ts",
                        "tsConfig": "apps/whatsapp-backend/tsconfig.app.json",
                        "assets": ["apps/whatsapp-backend/src/assets"]
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/whatsapp-backend/src/environments/environment.ts",
                                    "with": "apps/whatsapp-backend/src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "whatsapp-backend:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/whatsapp-backend/**/*.ts"]
                    },
                    "outputs": ["{options.outputFile}"]
