        "whatsapp-ui": {
            "projectType": "application",
            "root": "apps/whatsapp-ui",
            "sourceRoot": "apps/whatsapp-ui/src",
            "prefix": "whatsapp-clone",
            "architect": {
                "build": {
                    "builder": "@nrwl/angular:webpack-browser",
