{
    "version": 1,
    "projects": {
        "data": {
            "root": "libs/data",
            "sourceRoot": "libs/data/src",
            "projectType": "library",
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/data/**/*.ts"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/data"],
                    "options": {
                        "jestConfig": "libs/data/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "whatsapp-backend": {
            "root": "apps/whatsapp-backend",
            "sourceRoot": "apps/whatsapp-backend/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/node:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/whatsapp-backend",
                        "main": "apps/whatsapp-backend/src/main.ts",
                        "tsConfig": "apps/whatsapp-backend/tsconfig.app.json",
                        "assets": ["apps/whatsapp-backend/src/assets"]
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/whatsapp-backend/src/environments/environment.ts",
                                    "with": "apps/whatsapp-backend/src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "whatsapp-backend:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/whatsapp-backend/**/*.ts"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/whatsapp-backend"],
                    "options": {
                        "jestConfig": "apps/whatsapp-backend/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "whatsapp-ui": {
            "projectType": "application",
            "root": "apps/whatsapp-ui",
            "sourceRoot": "apps/whatsapp-ui/src",
            "prefix": "whatsapp-clone",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/whatsapp-ui",
                        "index": "apps/whatsapp-ui/src/index.html",
                        "main": "apps/whatsapp-ui/src/main.ts",
                        "polyfills": "apps/whatsapp-ui/src/polyfills.ts",
                        "tsConfig": "apps/whatsapp-ui/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "apps/whatsapp-ui/src/favicon.ico",
                            "apps/whatsapp-ui/src/assets"
                        ],
                        "styles": ["apps/whatsapp-ui/src/styles.scss"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "apps/whatsapp-ui/src/environments/environment.ts",
                                    "with": "apps/whatsapp-ui/src/environments/environment.prod.ts"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "whatsapp-ui:build:production"
                        },
                        "development": {
                            "browserTarget": "whatsapp-ui:build:development"
                        }
                    },
                    "defaultConfiguration": "development",
                    "options": {
                        "proxyConfig": "apps/whatsapp-ui/proxy.conf.json",
                        "browserTarget": "whatsapp-ui:build"
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "whatsapp-ui:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "apps/whatsapp-ui/src/**/*.ts",
                            "apps/whatsapp-ui/src/**/*.html"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/whatsapp-ui"],
                    "options": {
                        "jestConfig": "apps/whatsapp-ui/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "whatsapp-ui-e2e": {
            "root": "apps/whatsapp-ui-e2e",
            "sourceRoot": "apps/whatsapp-ui-e2e/src",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/whatsapp-ui-e2e/cypress.json",
                        "devServerTarget": "whatsapp-ui:serve:development",
                        "tsConfig": "apps/whatsapp-ui-e2e/tsconfig.json"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "whatsapp-ui:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "apps/whatsapp-ui-e2e/**/*.{js,ts}"
                        ]
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/angular",
        "packageManager": "npm"
    },
    "schematics": {
        "@nrwl/angular": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            },
            "storybook-configuration": {
                "linter": "eslint"
            }
        },
        "@nrwl/angular:application": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest"
        },
        "@nrwl/angular:component": {
            "style": "scss"
        }
    },
    "defaultProject": "whatsapp-ui"
}
